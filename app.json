{
  "expo": {
    "name": "dentfymobile",
    "slug": "dentfymobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A3446"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1A3446"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.izascr.dentfymobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "dentfymobile",
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8302ce70-4458-4e40-9442-db037ed8cfe9"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8302ce70-4458-4e40-9442-db037ed8cfe9"
    }
  }
}
